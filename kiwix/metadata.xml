<?xml version="1.0" encoding="utf-8"?>
<mirror-sites>
  <mirror-site id="wikipedia-kiwix">
    <name>Wikipedia Mirrored By Kiwix</name>
    <storage type="file"/>
    <advertiser type="kiwix"/>
    <advertiser type="httpdir"/>
    <advertiser type="rsync"/>
    <initializer>
      <executable>updater.py</executable>
    </initializer>
    <updater>
      <executable>updater.py</executable>
      <schedule type="interval">1d</schedule>
    </updater>
  </mirror-site>
  <mirror-site id="stackexchange-kiwix">
    <name>stackexchange Mirrored By Kiwix</name>
    <storage type="file"/>
    <advertiser type="httpdir"/>
    <advertiser type="ftp"/>
    <advertiser type="rsync"/>
    <advertiser type="kiwix"/>
    <initializer>
      <executable>updater.py</executable>
    </initializer>
    <updater>
      <executable>updater.py</executable>
      <schedule type="interval">1d</schedule>
    </updater>
  </mirror-site>
<mirror-sites/>